{
  "name": "smooth-scroller",
  "description": "Simple javascript smooth scrolling library",
  "homepage": "https://github.com/firstandthird/smooth-scroller",
  "author": "First+Third <code@firstandthird.com> (http://firstandthird.com/)",
  "license": "MIT",
  "version": "1.3.0",
  "main": "dist/smooth-scroller.js",
  "module": "index.js",
  "scripts": {
    "start": "scriptkit",
    "build": "scriptkit build",
    "dev": "scriptkit dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": "scriptkit"
  },
  "watch": {
    "index.js": [
      "lint",
      "build"
    ]
  },
  "keywords": [
    "browser",
    "scroll",
    "smooth"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/firstandthird/smooth-scroller.git"
  },
  "bugs": {
    "url": "https://github.com/firstandthird/smooth-scroller/issues"
  },
  "devDependencies": {
    "eslint-config-firstandthird": "3.2.0",
    "eslint-plugin-import": "2.2.0",
    "scriptkit": "0.2.0"
  },
  "scriptkit": {
    "test": {
      "enabled": false
    },
    "files": {
      "dist/smooth-scroller.js": "index.js"
    },
    "scripts": {
      "babel": {
        "exclude": [
          "node_modules/tape-rollup/*"
        ]
      }
    }
  },
  "eslintConfig": {
    "extends": "firstandthird",
    "parserOptions": {
      "sourceType": "module"
    },
    "env": {
      "browser": true
    }
  },
  "dependencies": {
    "domassist": "^1.9.0"
  }
}
