{
  "name": "smooth-scroller",
  "description": "Simple javascript smooth scrolling library",
  "homepage": "https://github.com/firstandthird/smooth-scroller",
  "author": "First+Third",
  "license": "MIT",
  "version": "1.0.1",
  "main": "dist/smooth-scroller.js",
  "scripts": {
    "build": "mkdir -p dist && babel index.js --out-file dist/smooth-scroller.js --source-maps",
    "dev": "concurrently -r 'nswatch' 'static-server -p 8000'",
    "lint": "eslint index.js",
    "prepublish": "npm run build"
  },
  "watch": {
    "index.js": ["lint", "build"]
  },
  "keywords": [
    "browser",
    "scroll",
    "smooth"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/firstandthird/smooth-scroller.git"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "concurrently": "^3.3.0",
    "eslint": "^3.5.0",
    "eslint-config-firstandthird": "^3.0.2",
    "eslint-plugin-import": "^2.2.0",
    "nswatch": "^0.2.0",
    "static-server": "^2.0.3"
  },
  "eslintConfig": {
    "extends": "firstandthird"
  },
  "babel": {
    "presets": ["es2015"]
  }
}
